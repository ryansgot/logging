<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto">
  <Button
      android:id="@+id/crashButton"
      android:text="Crash"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent" />
  <Button
      android:id="@+id/alarmButton"
      android:text="Dev Metrics Alarm"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@id/crashButton"
      app:layout_constraintStart_toStartOf="parent" />
  <Button
      android:id="@+id/watchButton"
      android:text="Dev Metrics Watch"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@id/alarmButton"
      app:layout_constraintStart_toStartOf="parent" />
  <Button
      android:id="@+id/infoButton"
      android:text="Dev Metrics Info"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@id/watchButton"
      app:layout_constraintStart_toStartOf="parent" />
  <Button
      android:id="@+id/logAnalyticsButton"
      android:text="Log Analytics"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@id/infoButton"
      app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>