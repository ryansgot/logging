<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fsryan.tools.loggingtestapp.appcenter">

    <application
        android:name="com.fsryan.tools.android.loggingtestapplib.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.fsryan.tools.android.loggingtestapplib.TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Notice that you can avoid the start overhead by adding this
           - provider to your AndroidManifest.xml -->
        <provider
            android:authorities="${applicationId}.appcenter.init"
            android:name="com.fsryan.tools.logging.android.AppCenterInitializationProvider"
            android:enabled="true"
            android:exported="false">
            <meta-data android:name="fsacsec" android:value="${testapp.acsecret}" />
        </provider>
    </application>
</manifest>
